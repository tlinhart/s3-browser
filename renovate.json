{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:daily",
    ":preserveSemverRanges"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": "eslint",
      "groupName": "eslint"
    },
    {
      "matchSourceUrlPrefixes": [
        "https://github.com/webpack/",
        "https://github.com/webpack-contrib/"
      ],
      "groupName": "webpack"
    }
  ]
}
